{
    "name": "aws-upload/aws-upload",
    "description": "aws-upload is a CLI Tool to manage rsync",
    "keywords": ["rsync","upload"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6.4",
        "wp-cli/php-cli-tools": "^0.11.2"
    },
    "require-dev": {
        "symfony/filesystem": "^3.2",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "AwsUpload\\": "src/AwsUpload"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwsUpload\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always test/AwsUpload",
        "cs": "phpcs --standard=PSR2 src/AwsUpload",
        "cbf": "phpcbf --standard=PSR2 src/AwsUpload"
    },
    "bin": ["aws-upload"]
}
